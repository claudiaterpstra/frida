  <section>
    <div class="block-card">
      <div class="wrapper">
        <div class="fridateam-card hwei archer wow slideInDown" data-wow-delay="<%= rand(0.5..1.0)%>s">
          <div class="fridateam-card__image fridateam-card__image--archer img-hwei">
          </div>
          <div class="fridateam-card__level fridateam-card__level--archer">Back-end Ninja</div>
          <div class="fridateam-card__unit-name">Hwei</div>
          <div class="fridateam-card__unit-description">
            <p>Lorem ipsum dolor sit amet, consectetur adipisicing elit. Minima mollitia amet quaerat numquam dolorum, esse consequuntur sit.</p>
          </div>

          <div class="fridateam-card__unit-stats fridateam-card__unit-stats--archer clearfix">
            <div class="one-third">
              <div class="stat"><%= link_to "<i class='fa fa-facebook' aria-hidden='true'></i>".html_safe, "http://www.facebook.com/hwei.tan", target: :_blank %></div>
              <div class="stat-value"></div>
            </div>

            <div class="one-third">
              <div class="stat"><%= link_to "<i class='fa fa-github-alt' aria-hidden='true'></i>".html_safe, "http://www.github.com/hw3i", target: :_blank %></div>
              <div class="stat-value"></div>
            </div>

            <div class="one-third no-border">
              <div class="stat"><%= link_to "<i class='fa fa-instagram' aria-hidden='true'></i>".html_safe, "http://www.instagram.com/hw3i", target: :_blank %></div>
              <div class="stat-value"></div>
            </div>
          </div>
        </div>

        <div class="fridateam-card claudia archer wow slideInDown" data-wow-delay="<%= rand(0.5..1.0)%>s">
          <div class="fridateam-card__image fridateam-card__image--archer img-claudia">
          </div>
          <div class="fridateam-card__level fridateam-card__level--archer">Dutch Captain</div>
          <div class="fridateam-card__unit-name">Claudia</div>
          <div class="fridateam-card__unit-description">
            <p>Lorem ipsum dolor sit amet, consectetur adipisicing elit. Minima mollitia amet quaerat numquam dolorum, esse consequuntur sit.</p>
          </div>

          <div class="fridateam-card__unit-stats fridateam-card__unit-stats--archer clearfix">
            <div class="one-third">
              <div class="stat"><%= link_to "<i class='fa fa-facebook' aria-hidden='true'></i>".html_safe, "http://www.facebook.com/claudia.terpstra.39", target: :_blank %></div>
              <div class="stat-value"></div>
            </div>

            <div class="one-third">
              <div class="stat"><%= link_to "<i class='fa fa-github-alt' aria-hidden='true'></i>".html_safe, "http://www.github.com/claudiaterpstra", target: :_blank %></div>
              <div class="stat-value"></div>
            </div>

            <div class="one-third no-border">
              <div class="stat"><%= link_to "<i class='fa fa-instagram' aria-hidden='true'></i>".html_safe, "http://www.instagram.com/claudia.terpstra", target: :_blank %></div>
              <div class="stat-value"></div>
            </div>
          </div>
        </div>

        <div class="fridateam-card bekah archer wow slideInDown" data-wow-delay="<%= rand(0.5..1.0)%>s">
          <div class="fridateam-card__image fridateam-card__image--archer img-bekah">
            <%# image_tag 'http://res.cloudinary.com/rebekah/image/upload/v1497456893/fridaprofilepic_klwang.png' %>
          </div>
          <div class="fridateam-card__level fridateam-card__level--archer">Design Bunny</div>
          <div class="fridateam-card__unit-name">Rebekah</div>
          <div class="fridateam-card__unit-description">
            <p>Lorem ipsum dolor sit amet, consectetur adipisicing elit. Minima mollitia amet quaerat numquam dolorum, esse consequuntur sit.</p>
          </div>

          <div class="fridateam-card__unit-stats fridateam-card__unit-stats--archer clearfix">
            <div class="one-third">
              <div class="stat"><%= link_to "<i class='fa fa-facebook' aria-hidden='true'></i>".html_safe, "http://www.facebook.com/rebekahcheng", target: :_blank %></div>
              <div class="stat-value"></div>
            </div>

            <div class="one-third">
              <div class="stat"><%= link_to "<i class='fa fa-github-alt' aria-hidden='true'></i>".html_safe, "http://www.github.com/rebekahcheng", target: :_blank %></div>
              <div class="stat-value"></div>
            </div>

            <div class="one-third no-border">
              <div class="stat"><%= link_to "<i class='fa fa-instagram' aria-hidden='true'></i>".html_safe, "http://www.instagram.com/rebekahcheng", target: :_blank %></div>
              <div class="stat-value"></div>
            </div>
          </div>
        </div>

      </div>
    </div>
  </section>

  <section>
    <div class="parallax-three">
      <h2></h2>
    </div>
  </section>

  <section>
    <div class="block">
      <div class="row">
        <div class="title" data-anchor="about" >
          <h3>Our Honorary Frida Members</h3>
          <h1>THANK YOU</h1>
          <i class="em em-raised_hands"></i>
        </div>
        <div class="col-xs-6 col-xs-offset-3 thankyou">
          <h2><%= link_to "Alex", "http://www.github.com/alex-felix", target: :_blank %>,
          <%= link_to "Arthur", "http://www.github.com/arthur-littm", target: :_blank %>,
          <%= link_to "Boris", "http://www.github.com/papillard", target: :_blank %>,
          <%= link_to "Ed", "http://www.github.com/eschultz", target: :_blank %>,
          <%= link_to "Guv", "http://www.github.com/guv0", target: :_blank %>,
          <%= link_to "Romain", "http://www.github.com/lacampe", target: :_blank %>,
          <%= link_to "Remi", "http://www.github.com/dyvoire", target: :_blank %>,
          <%= link_to "Thomas", "http://www.github.com/thomassertorio", target: :_blank %>.
          </h2>
          <p></p>
        </div>
      </div>
      <div class="row">
      <%= link_to "#about", class: "floating scroll-center" do %>
        <%= image_tag 'arrowup.png' %>
        <p id="scroll-text">BACK TO TOP</p>
      <% end %>
      </div>
    </div>
  </section>
<!-- </div> -->

<% content_for(:after_js) do %>
  <script>
  $("a[href='#about']").click(function() {
    $("html, body").animate({ scrollTop: 0 }, 2500);
    return false;
  });
  </script>
<% end %>

